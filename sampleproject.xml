<!DOCTYPE projector SYSTEM "projector.dtd"><projector>	<project>		<name>Softlogger</name>		<projectPackage>ca.brood.softlogger</projectPackage>		<rootElement>softlogger</rootElement>		<rootObject>Softlogger</rootObject>	</project>	<object>		<name>Softlogger</name>		<reference>			<name>DataServer</name>			<elementName>server</elementName>			<targetType>DataServer</targetType>			<relationship>onetoone</relationship>		</reference>		<reference>			<name>SoftloggerChannels</name>			<elementName>channel</elementName>			<targetType>SoftloggerChannel</targetType>			<relationship>onetomany</relationship>		</reference>	</object>	<object>		<name>DataServer</name>		<field>			<name>host</name>			<elementName>host</elementName>			<type>string</type>			<size>255</size>		</field>	</object>	<object>		<name>SoftloggerChannel</name>		<field>			<name>id</name>			<elementName>id</elementName>			<type>integer</type>			<size>4</size>			<modifier>ro</modifier>		</field>	</object></projector>