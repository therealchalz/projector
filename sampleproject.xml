<!DOCTYPE projector SYSTEM "projector.dtd"><projector>	<project>		<name>Softlogger</name>		<projectPackage>ca.brood.softlogger</projectPackage>		<rootElement>softlogger</rootElement>		<rootObject>Softlogger</rootObject>	</project>	<object>		<name>Softlogger</name>		<field>			<name>name</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>poll</name>			<type>integer</type>			<size>4</size>		</field>		<field>			<name>tableFilePath</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>dataFilePath</name>			<type>string</type>			<size>255</size>		</field>				<reference>			<name>dataServer</name>			<elementName>server</elementName>			<targetType>DataServer</targetType>			<relationship>onetoone</relationship>		</reference>		<reference>			<name>softloggerChannels</name>			<elementName>channel</elementName>			<targetType>SoftloggerChannel</targetType>			<relationship>onetomany</relationship>		</reference>	</object>	<object>		<name>DataServer</name>		<field>			<name>host</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>user</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>configPoll</name>			<type>integer</type>			<size>4</size>		</field>		<field>			<name>path</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>password</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>keyfile</name>			<type>string</type>			<size>255</size>		</field>	</object>	<object>		<name>SoftloggerChannel</name>		<reference>			<name>modbusChannel</name>			<targetType>ModbusChannel</targetType>			<subclassType>				<elementName>tcp</elementName>				<targetType>ModbusTcpChannel</targetType>			</subclassType>			<relationship>onetoone</relationship>		</reference>	</object>	<object>		<name>ModbusChannel</name>		<field>			<name>poll</name>			<type>integer</type>			<size>4</size>		</field>	</object>	<object>		<name>ModbusTcpChannel</name>		<superclass>ModbusChannel</superclass>		<field>			<name>port</name>			<type>integer</type>			<size>2</size>		</field>		<field>			<name>host</name>			<type>string</type>			<size>255</size>		</field>	</object></projector>