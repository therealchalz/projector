<!DOCTYPE projector SYSTEM "projector.dtd"><projector>	<project>		<name>Softlogger</name>		<projectPackage>ca.brood.softlogger</projectPackage>		<rootElement>softlogger</rootElement>		<rootObject>Softlogger</rootObject>	</project>	<object>		<name>Softlogger</name>		<field>			<name>name</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>poll</name>			<type>integer</type>			<size>4</size>		</field>		<field>			<name>tableFilePath</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>dataFilePath</name>			<type>string</type>			<size>255</size>		</field>				<reference>			<name>dataServer</name>			<elementName>server</elementName>			<targetType>DataServer</targetType>			<relationship>onetoone</relationship>		</reference>		<reference>			<name>softloggerChannels</name>			<elementName>channel</elementName>			<targetType>SoftloggerChannel</targetType>			<relationship>onetomany</relationship>		</reference>	</object>	<object>		<name>DataServer</name>		<field>			<name>host</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>user</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>configPoll</name>			<type>integer</type>			<size>4</size>		</field>		<field>			<name>path</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>password</name>			<type>string</type>			<size>255</size>		</field>		<field>			<name>keyfile</name>			<type>string</type>			<size>255</size>		</field>	</object>	<object>		<name>SoftloggerChannel</name>	</object>	<object>		<name>ModbusChannel</name>	</object></projector>